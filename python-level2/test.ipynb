    "s = []\n",
    "n = [1,2,3,4]\n",
    "\n",
    "def subsets(nums):\n",
    "        subset = []\n",
    "        if len(nums)==1:\n",
    "            return [nums[0]]\n",
    "        for i in range(len(nums)):\n",
    "            subset.extend(subsets(nums[0:i]))\n",
    "            subset.extend(subsets(nums[i+1:]))\n",
    "        return subset\n",
    "subsets(n)"
